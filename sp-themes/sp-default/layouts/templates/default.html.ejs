<nav id="header" class="navbar navbar-<%=themeconfig && themeconfig.preference && themeconfig.preference.headercolorscheme ? themeconfig.preference.headercolorscheme : 'default' %> navbar-fixed-top">
	<%- include(templatedir + "/header.html.ejs" ) %>
</nav>
<div class="container-fluid">
	<div class="row notification" id="notification">
		<div class="col-sm-12">
			<div id="curtime" class='label label-info pull-right'>
				<i class='glyphicon glyphicon-time'></i> 
				<span class='time'>
					<script>
						document.write((new Date()).toLocaleDateString() + ' ' + (new Date()).toLocaleTimeString())
					</script>
				</span>
			</div>
		</div>
	</div>
	<div class="row content">
		<div class="col-sm-2 sidebar" id="sidepanel" role="navbar"></div>
		<div class="col-sm-10 main" id="main" role="main">
			<div class='loading'></div>
		</div>
		<div class="col-sm-9 main" id="subform"></div>
	</div>
</div>
<footer class="footer navbar-<%=themeconfig && themeconfig.preference  && themeconfig.preference.footercolorscheme ?  themeconfig.preference.footercolorscheme : 'default navbar-fixed-bottom' %>">
	<%- include(templatedir + "/footer.html.ejs" ) %>
</footer>