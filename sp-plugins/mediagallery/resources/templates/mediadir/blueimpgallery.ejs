<%if(result && result.length > 0){%>
	<h2>Lightbox image gallery</h2>
	<!-- The container for the list of example images -->
	<div id="links" class="links">
		<%for(var i in result){var gallery =result[i];%>
			<%for(var p in gallery.photos){var photo =gallery.photos[p];%>
			<a href="<%= gallery.uri%><%= photo.file %>" title="<%= photo.display %>" data-gallery>
	        	<img src="<%= gallery.uri%><%= photo.file %>" alt="<%= photo.display %>">
	    	</a>
	    <%	}}%>
	</div>
	<!-- The Gallery as lightbox dialog, should be a child element of the document body -->
	<div id="blueimp-gallery" class="blueimp-gallery">
	    <div class="slides"></div>
	    <h3 class="title"></h3>
	    <a class="prev">‹</a>
	    <a class="next">›</a>
	    <a class="close">×</a>
	    <a class="play-pause"></a>
	    <ol class="indicator"></ol>
	</div>
<%}%>